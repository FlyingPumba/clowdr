        "directory": "packages/component-clients"
    },
    "bugs": {
        "url": "https://github.com/clowdr-app/clowdr/issues"
    },
    "homepage": "https://github.com/clowdr-app/clowdr#readme",
    "dependencies": {
        "@socket.io/redis-emitter": "^4.1.0",
        "@types/amqplib": "^0.8.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.12",
        "@types/generic-pool": "^3.1.10",
        "@types/jsonwebtoken": "^8.5.3",
        "@types/jwk-to-pem": "^2.0.0",
        "@types/node-fetch": "^2.5.10",
        "@types/redis": "^2.8.30",
        "@types/redlock": "^4.0.2",
        "@types/uuid": "^8.3.0",
        "@types/web-push": "^3.3.1",
        "@types/ws": "^7.4.5",
        "@urql/core": "^2.3.3",
        "@urql/exchange-retry": "^0.3.0",
        "amqplib": "^0.8.0",
        "assert": "^2.0.0",
        "bufferutil": "^4.0.3",
        "cors": "^2.8.5",
        "cpy-cli": "^3.1.1",
        "express": "^4.17.1",
        "generic-pool": "^3.8.2",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.5",
        "jwks-rsa": "^2.0.3",
        "node-env-run": "^4.0.2",
        "node-fetch": "^2.6.1",
        "redis": "^3.1.2",
        "redlock": "^4.2.0",
        "rimraf": "^3.0.2",
        "subscriptions-transport-ws": "^0.9.19",
        "ts-patch": "^1.4.5",
        "ttypescript": "^1.5.12",
        "typescript": "^4.3.5",
        "typescript-is": "^0.18.3",
        "utf-8-validate": "^5.0.5",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "tsc-watch": "^4.4.0"
    },
    "engines": {
        "node": ">=16.x",
        "pnpm": "^6.19.x"
    },
    "exports": {
        "./*": {
            "import": "./build/esm/*.js",
            "require": "./build/cjs/*.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "build/esm/*"
            ]
        }
    }
}
