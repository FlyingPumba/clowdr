{
    "name": "@midspace/auth",
    "version": "0.0.0",
    "scripts": {
        "build": "pnpm build:cjs && pnpm build:esm",
        "build:cjs": "tsc --build tsconfig.cjs.json",
        "build:esm": "tsc --build tsconfig.esm.json",
        "clean": "rimraf build",
        "prepare": "ts-patch install -s",
        "codegen": "graphql-codegen --config codegen.js"
    },
    "author": "Clowdr CIC",
    "license": "AGPL-3.0-or-later",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clowdr-app/clowdr.git",
        "directory": "packages/auth"
    },
    "bugs": {
        "url": "https://github.com/clowdr-app/clowdr/issues"
    },
    "homepage": "https://github.com/clowdr-app/clowdr#readme",
    "dependencies": {
        "@midspace/caches": "workspace:*",
        "@midspace/component-clients": "workspace:*",
        "@midspace/graphql": "workspace:*",
        "@midspace/shared-types": "workspace:*",
        "@socket.io/redis-emitter": "^4.1.0",
        "@types/amqplib": "^0.8.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.12",
        "@types/generic-pool": "^3.1.10",
        "@types/jsonwebtoken": "^8.5.3",
        "@types/jwk-to-pem": "^2.0.0",
        "@types/node-fetch": "^2.5.10",
        "@types/redis": "^2.8.30",
        "@types/redlock": "^4.0.2",
        "@types/uuid": "^8.3.0",
        "@types/web-push": "^3.3.1",
        "@types/ws": "^7.4.5",
        "@urql/core": "^2.3.3",
        "@urql/exchange-retry": "^0.3.0",
        "amqplib": "^0.8.0",
        "assert": "^2.0.0",
        "bufferutil": "^4.0.3",
        "cors": "^2.8.5",
        "cpy-cli": "^3.1.1",
        "express": "^4.17.1",
        "express-jwt": "^6.1.0",
        "generic-pool": "^3.8.2",
        "graphql": "^15.7.2",
        "graphql-tag": "^2.12.5",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.5",
        "jwks-rsa": "^2.0.3",
        "node-env-run": "^4.0.2",
        "node-fetch": "^2.6.1",
        "pino": "^7.1.0",
        "redis": "^3.1.2",
        "redlock": "^4.2.0",
        "rimraf": "^3.0.2",
        "socket.io": "^3.1.2",
        "subscriptions-transport-ws": "^0.9.19",
        "ts-patch": "^2.0.1",
        "typescript": "^4.5.2",
        "typescript-is": "^0.18.3",
        "utf-8-validate": "^5.0.5",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.2.2",
        "@graphql-codegen/fragment-matcher": "^3.2.0",
        "@graphql-codegen/introspection": "^2.1.0",
        "@graphql-codegen/plugin-helpers": "^2.3.0",
        "@graphql-codegen/typed-document-node": "^2.2.0",
        "@graphql-codegen/typescript": "^2.3.0",
        "@graphql-codegen/typescript-document-nodes": "^2.2.0",
        "@graphql-codegen/typescript-graphql-files-modules": "^2.1.0",
        "@graphql-codegen/typescript-graphql-request": "^4.2.0",
        "@graphql-codegen/typescript-operations": "^2.2.0",
        "@graphql-codegen/typescript-urql": "^3.3.0",
        "tsc-watch": "^4.4.0"
    },
    "engines": {
        "node": ">=16.x",
        "pnpm": "^6.19.x"
    },
    "exports": {
        "./*": {
            "import": "./build/esm/*.js",
            "require": "./build/cjs/*.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "build/cjs/*",
                "build/esm/*"
            ]
        }
    }
}
