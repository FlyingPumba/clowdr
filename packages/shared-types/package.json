{
    "name": "@midspace/shared-types",
    "scripts": {
        "build": "pnpm build:cjs && pnpm build:esm",
        "build:cjs": "tsc --build tsconfig.json",
        "build:esm": "tsc --build tsconfig.esm.json",
        "clean": "rimraf build",
        "prepare": "ts-patch install -s"
    },
    "author": "Clowdr CIC",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clowdr-app/clowdr.git",
        "directory": "packages/shared-types"
    },
    "bugs": {
        "url": "https://github.com/clowdr-app/clowdr/issues"
    },
    "homepage": "https://github.com/clowdr-app/clowdr#readme",
    "dependencies": {
        "@types/ramda": "^0.27.42",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "jsonata": "^1.8.4",
        "ramda": "^0.27.1",
        "ts-patch": "^1.4.5",
        "typescript": "^4.3.5",
        "typescript-is": "^0.18.3"
    },
    "engines": {
        "node": ">=16.x",
        "pnpm": "^6.19.x"
    },
    "exports": {
        "./*": {
            "import": "./build/esm/*.js",
            "require": "./build/cjs/*.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "build/cjs/*",
                "build/esm/*"
            ]
        }
    },
    "devDependencies": {
        "rimraf": "^3.0.2"
    }
}
