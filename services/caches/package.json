{
    "name": "@midspace/service-caches",
    "private": true,
    "scripts": {
        "build": "tsc --build && pnpm copy-resources",
        "clean": "rimraf build",
        "copy-resources": "rimraf build/resources && cpy --cwd=src/resources \"**/*\" ../../build/resources --parents",
        "start": "node --enable-source-maps -r dotenv/config build/web-server.js | pino-pretty -t SYS:HH:MM:ss.l -S -i hostname,pid -L trace",
        "dev": "pnpm copy-resources && tsc-watch -b --onSuccess \"pnpm start\"",
        "test-publisher": "node-env-run -E ./src/test/.publisher.env -e \"ts-node ./src/test/publisher.ts\"",
        "test-consumer": "node-env-run -E ./src/test/.consumer.env -e \"ts-node ./src/test/consumer.ts\""
    },
    "author": "Clowdr CIC",
    "license": "AGPL-3.0-or-later",
    "dependencies": {
        "@midspace/auth": "workspace:*",
        "@midspace/caches": "workspace:*",
        "@midspace/component-clients": "workspace:*",
        "@midspace/server-utils": "workspace:*",
        "@socket.io/redis-emitter": "^4.1.0",
        "@types/amqplib": "^0.8.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/generic-pool": "^3.1.10",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/jwk-to-pem": "^2.0.1",
        "@types/node-fetch": "^2.5.10",
        "@types/ramda": "^0.27.64",
        "@types/redis": "^2.8.32",
        "@types/redlock": "^4.0.2",
        "@types/uuid": "^8.3.1",
        "@types/web-push": "^3.3.2",
        "@urql/core": "^2.4.1",
        "@urql/exchange-retry": "^0.3.2",
        "amqplib": "^0.8.0",
        "assert": "^2.0.0",
        "body-parser": "^1.19.1",
        "bufferutil": "^4.0.5",
        "cors": "^2.8.5",
        "cpy-cli": "^3.1.1",
        "express": "^4.17.1",
        "generic-pool": "^3.8.2",
        "graphql": "^15.7.2",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.5",
        "jwks-rsa": "^2.0.5",
        "node-env-run": "^4.0.2",
        "node-fetch": "^2.6.6",
        "pino": "^7.6.4",
        "pino-http": "^6.6.0",
        "ramda": "^0.27.1",
        "redis": "^3.1.2",
        "redlock": "^4.2.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.5",
        "typescript-is": "^0.19.0",
        "utf-8-validate": "^5.0.7",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "dotenv": "^10.0.0",
        "pino-pretty": "^7.2.0",
        "tsc-watch": "^4.5.0"
    },
    "engines": {
        "node": ">=16.x",
        "pnpm": "^6.19.x"
    }
}
