{
    "name": "clowdr-presence",
    "version": "1.0.0",
    "description": "Presence counters",
    "main": "index.js",
    "engines": {
        "node": "15.11.x"
    },
    "scripts": {
        "build": "npm run build-shared && npm run build-this && npm run-script copy-resources",
        "build-this": "ttsc",
        "build-shared": "npm run --prefix=../../shared/ --cwd=../../shared/ build",
        "copy-resources": "rimraf build/resources && cpy --cwd=src/resources \"**/*\" ../../build/resources --parents",
        "start": "node build/server.js",
        "dev-stage2": "tsc-watch --compiler \"ttypescript/bin/tsc\" --onSuccess \"npm run-script --prefix=../../ --cwd=../../ run-services-realtime\"",
        "dev": "npm run build-shared && node-env-run -E ./.env -e \"npm run dev-stage2\"",
        "test-publisher": "node-env-run -E ./src/test/.publisher.env -e \"ts-node ./src/test/publisher.ts\"",
        "test-consumer": "node-env-run -E ./src/test/.consumer.env -e \"ts-node ./src/test/consumer.ts\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/clowdr/clowdr-app.git"
    },
    "author": "Clowdr CIC",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/clowdr/clowdr-app/issues"
    },
    "homepage": "https://github.com/clowdr/clowdr-app#readme",
    "dependencies": {
        "@socket.io/redis-emitter": "^4.0.0",
        "amqplib": "^0.7.1",
        "assert": "^2.0.0",
        "bufferutil": "^4.0.3",
        "cpy-cli": "^3.1.1",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.4",
        "jwks-rsa": "^2.0.2",
        "node-env-run": "^4.0.2",
        "node-fetch": "^2.6.1",
        "redis": "^3.0.2",
        "redlock": "^4.2.0",
        "rimraf": "^3.0.2",
        "socket.io": "^3.1.1",
        "socket.io-client": "^4.0.0",
        "socket.io-redis": "^6.0.1",
        "ttypescript": "^1.5.12",
        "typescript": "^4.1.5",
        "typescript-is": "^0.18.2",
        "utf-8-validate": "^5.0.4",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/amqplib": "^0.5.17",
        "@types/express": "^4.17.11",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/jwk-to-pem": "^2.0.0",
        "@types/node-fetch": "^2.5.8",
        "@types/redis": "^2.8.28",
        "@types/redlock": "^4.0.1",
        "@types/socket.io": "^2.1.13",
        "@types/socket.io-redis": "^1.0.26",
        "@types/uuid": "^8.3.0",
        "tsc-watch": "^4.2.9"
    }
}
