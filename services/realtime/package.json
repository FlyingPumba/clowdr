        "utf-8-validate": "^5.0.5",
        "uuid": "^8.3.2",
        "web-push": "^3.4.5"
    },
    "devDependencies": {
        "tsc-watch": "^4.4.0"
    },
    "engines": {
        "node": ">=16.x",
        "pnpm": "^6.19.x"
    }
}
