{
    "name": "@midspace/service-realtime",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "tsc --build && pnpm copy-resources",
        "clean": "rimraf build",
        "copy-resources": "rimraf build/resources && cpy --cwd=src/resources \"**/*\" ../../build/resources --parents",
        "start": "pnpm nf start --procfile Procfile -p 3002 | pino-pretty -t SYS:HH:MM:ss.l -S -i hostname,pid",
        "dev": "pnpm copy-resources && tsc-watch -b --onSuccess \"pnpm start\"",
        "codegen": "pnpm exec graphql-codegen --config codegen.js",
        "test-publisher": "node-env-run -E ./src/test/.publisher.env -e \"ts-node ./src/test/publisher.ts\"",
        "test-consumer": "node-env-run -E ./src/test/.consumer.env -e \"ts-node ./src/test/consumer.ts\""
    },
    "author": "Clowdr CIC",
    "license": "AGPL-3.0-or-later",
    "dependencies": {
        "@graphql-codegen/cli": "^1.21.6",
        "@graphql-codegen/fragment-matcher": "^2.0.1",
        "@graphql-codegen/introspection": "^1.18.2",
        "@graphql-codegen/typed-document-node": "^1.18.9",
        "@graphql-codegen/typescript": "^1.22.4",
        "@graphql-codegen/typescript-document-nodes": "^1.17.15",
        "@graphql-codegen/typescript-graphql-files-modules": "^1.18.1",
        "@graphql-codegen/typescript-graphql-request": "^3.2.4",
        "@graphql-codegen/typescript-operations": "^1.18.3",
        "@graphql-codegen/typescript-urql": "^3.2.2",
        "@graphql-typed-document-node/core": "^3.1.0",
        "@midspace/auth": "workspace:*",
        "@midspace/caches": "workspace:*",
        "@midspace/component-clients": "workspace:*",
        "@midspace/graphql": "workspace:*",
        "@socket.io/redis-emitter": "^4.1.0",
        "@types/amqplib": "^0.8.0",
        "@types/express": "^4.17.12",
        "@types/jsonwebtoken": "^8.5.3",
        "@types/jwk-to-pem": "^2.0.0",
        "@types/node-fetch": "^2.5.10",
        "@types/redis": "^2.8.30",
        "@types/redlock": "^4.0.1",
        "@types/socket.io": "^3.0.1",
        "@types/socket.io-redis": "^1.0.27",
        "@types/uuid": "^8.3.0",
        "@types/web-push": "^3.3.1",
        "@types/ws": "^7.4.5",
        "@urql/core": "^2.3.4",
        "amqplib": "^0.8.0",
        "assert": "^2.0.0",
        "async-mutex": "^0.3.2",
        "bufferutil": "^4.0.3",
        "cpy-cli": "^3.1.1",
        "cross-fetch": "^3.1.4",
        "express": "^4.17.1",
        "generic-pool": "^3.8.2",
        "graphql": "^15.7.2",
        "graphql-tag": "^2.12.5",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.5",
        "jwks-rsa": "^2.0.3",
        "node-env-run": "^4.0.2",
        "node-fetch": "^2.6.1",
        "pino": "^7.1.0",
        "pino-pretty": "^7.2.0",
        "redis": "^3.1.2",
        "redlock": "^4.2.0",
        "rimraf": "^3.0.2",
        "socket.io": "^3.1.2",
        "socket.io-client": "^4.1.2",
        "socket.io-redis": "^6.0.1",
        "subscriptions-transport-ws": "^0.9.19",
        "ttypescript": "^1.5.12",
