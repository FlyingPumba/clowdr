{
    "name": "@midspace/service-importer",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "tsc --build && pnpm copy-resources",
        "clean": "rimraf build",
        "copy-resources": "rimraf build/resources && cpy --cwd=src/resources \"**/*\" ../../build/resources --parents",
        "start": "concurrently -n s: pnpm:start:*",
        "start:web": "node --enable-source-maps -r dotenv/config build/web-server.js | pino-pretty -t SYS:HH:MM:ss.l -S -i hostname,pid",
        "start:tasksWorker": "node --enable-source-maps -r dotenv/config build/workers/tasks.js | pino-pretty -t SYS:HH:MM:ss.l -S -i hostname,pid",
        "dev": "pnpm copy-resources && tsc-watch -b --onSuccess \"pnpm start\"",
        "codegen": "pnpm exec graphql-codegen --config codegen.js"
    },
    "author": "Clowdr CIC",
    "license": "AGPL-3.0-or-later",
    "dependencies": {
        "@graphql-typed-document-node/core": "^3.1.1",
        "@midspace/component-clients": "workspace:*",
        "@midspace/graphql": "workspace:*",
        "@midspace/hasura": "workspace:*",
        "@midspace/server-utils": "workspace:*",
        "@midspace/shared-types": "workspace:*",
        "@socket.io/redis-emitter": "^4.1.0",
        "@types/amqplib": "^0.8.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/generic-pool": "^3.1.10",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/jwk-to-pem": "^2.0.1",
        "@types/node-fetch": "^2.5.12",
        "@types/ramda": "^0.27.64",
        "@types/redis": "^2.8.32",
        "@types/redlock": "^4.0.2",
        "@types/uuid": "^8.3.4",
        "@types/web-push": "^3.3.2",
        "@types/ws": "^8.2.2",
        "@urql/core": "^2.4.1",
        "@urql/exchange-retry": "^0.3.2",
        "amqplib": "^0.8.0",
        "assert": "^2.0.0",
        "async-mutex": "^0.3.2",
        "body-parser": "^1.19.1",
        "bufferutil": "^4.0.6",
        "cors": "^2.8.5",
        "cpy-cli": "^3.1.1",
        "express": "^4.17.1",
        "generic-pool": "^3.8.2",
        "graphql": "^15.7.2",
        "graphql-tag": "^2.12.6",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.5",
        "jwks-rsa": "^2.0.5",
        "node-env-run": "^4.0.2",
        "node-fetch": "^2.6.7",
        "pino": "^7.6.4",
        "pino-http": "^6.6.0",
        "ramda": "^0.28.0",
        "redis": "^3.1.2",
        "redlock": "^4.2.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.5",
        "typescript-is": "^0.19.0",
        "utf-8-validate": "^5.0.8",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.5.0",
        "@graphql-codegen/typescript": "^2.4.3",
        "@graphql-codegen/typescript-operations": "^2.2.4",
        "@graphql-codegen/typescript-urql": "^3.5.1",
        "@graphql-typed-document-node/core": "^3.1.1",
        "concurrently": "^6.4.0",
        "dotenv": "^16.0.0",
        "pino-pretty": "^7.2.0"
    },
    "engines": {
        "node": ">=17.x"
    }
}
